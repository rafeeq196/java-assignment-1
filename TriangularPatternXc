package assignment;

public class TriangularPatternXc {

	public static void main(String[] args) {
		int n=8;
		for(int row=1; row<=n; row++) {
			for(int col=1; col<=row; col++) {
				System.out.print("  ");}
			for(int col=row; col<=n; col++) {
				System.out.print(" #");
			}System.out.println();
		}
     
	}

}

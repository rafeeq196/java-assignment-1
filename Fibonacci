package assignment;

public class Fibonacci {

	public static void main(String[] args) {
		int z=0;
		int n=0;
		int fn;// f(n) to b computed
		int fnMinus1=1;//f(n-1)
		int fnMinus2=1;//f(n-2)
		int nMax = 20;
		int sum = fnMinus1+fnMinus2;
		double average;
		average=sum/nMax;
		System.out.println("The first " + nMax + " Fibonacci numbers are :");
		while(n<=nMax) {
			++n;
			System.out.print(z+" ");
			fnMinus1=fnMinus2;
			fnMinus2=z;
			z=fnMinus1+fnMinus2;
			
		}
		System.out.println("The avarage is :"+average);


	}

}
